              detalle: "<%= products[3].descripcion %>",
              color: "<%= products[3].color %>",
              material: "<%= products[3].material %>",
              talle: ['L']
            }
          }
        ];
        console.log(productosDB);
        productos = productosDB;
      }

      loadProducts();
      console.log(productos);


