            descripcion: {
              detalle: "<%= products[2].descripcion %>",
              color: "<%= products[2].color %>",
              material: "<%= products[2].material %>",
              talle: ['M', 'L', 'XL']
            }
          },
          {
            id: <%= products[3].id %>,
            marca: "<%= products[3].marca %>",
            nombre: "<%= products[3].nombre %>",
            codigo: "<%= products[3].codigo %>",
